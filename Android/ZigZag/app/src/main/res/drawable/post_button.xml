<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Main circular shape -->
    <item>
        <shape android:shape="oval">
            <size android:width="100dp" android:height="100dp"/> <!-- Circle size -->
            <solid android:color="@color/blue"/> <!-- Circle color -->
        </shape>
    </item>

    <!-- Vertical line of the plus sign -->
    <item android:top="50dp" android:bottom="50dp" android:left="25dp" android:right="25dp">
        <shape android:shape="rectangle">
            <size android:width="10dp" android:height="10dp"/> <!-- Adjust size as needed -->
            <solid android:color="@color/white"/> <!-- Plus sign color -->
        </shape>
    </item>

    <!-- Horizontal line of the plus sign -->
    <item android:left="50dp" android:right="50dp" android:top="25dp" android:bottom="25dp">
        <shape android:shape="rectangle">
            <size android:width="10dp" android:height="10dp"/> <!-- Adjust size as needed -->
            <solid android:color="@color/white"/> <!-- Plus sign color -->
        </shape>
    </item>

</layer-list>
